<Window x:Class="Prb.Variables.Operators.wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prb.Variables.Operators.wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <ListBox x:Name="lstOperations" d:ItemsSource="{d:SampleData ItemCount=5}" SelectionChanged="lstOperations_SelectionChanged" Margin="0,0,0,10" Width="800"/>

    </Grid>
</Window>
