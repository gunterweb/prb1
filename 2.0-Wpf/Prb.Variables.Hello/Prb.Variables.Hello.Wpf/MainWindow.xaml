<Window x:Class="Prb.Variables.Hello.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prb.Variables.Hello.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtUserName" HorizontalAlignment="Left" Margin="54,83,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblUser" Content="Label" HorizontalAlignment="Left" Margin="54,52,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblUpperCase" Content="Label" HorizontalAlignment="Left" Margin="54,162,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnOK" Content="OK" HorizontalAlignment="Left" Margin="54,119,0,0" VerticalAlignment="Top" Width="120" Height="38" Click="BtnOK_Click"/>
        <Label x:Name="lblFeedback" Content="Label" HorizontalAlignment="Left" Margin="54,193,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
